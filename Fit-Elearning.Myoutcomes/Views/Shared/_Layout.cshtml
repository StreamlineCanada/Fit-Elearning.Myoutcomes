        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-53592313-12');
    </script>


    @{

        var ui = Fit_Elearning.MyOutcomes.Helper.UtilitiesHelper.GetUserInfo(Request.Cookies["ExtServerId"].Value);
        string displayName = ui.userId;
        bool isAdmin = Fit_Elearning.MyOutcomes.Helper.UtilitiesHelper.GetIsAdmin();

    }
    <link href="../../Content/ePlatform_style.css" type="text/css" rel="stylesheet">
</head>

    <body>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-6 col-xs-12 col-sm-12">
                <div class="logo"><img src="../../Images/FIT-logo.png" /></div>
            </div>
        </div>

            <nav class="navbar navbar-default">

                    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
