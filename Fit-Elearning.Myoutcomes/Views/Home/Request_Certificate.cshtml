@{
    ViewBag.Title = "Certificate of Course Completion";
    Layout = "~/Views/Shared/_Print.cshtml";
}

@using Fit_Elearning.MyOutcomes.Models
@model Fit_Elearning.MyOutcomes.Models.Certificate


    <div id="certificate">
        <img src="../../Images/@Model.certificateImageUrl" style="z-index:0;position:absolute;" />
        <div style="position: absolute; z-index: 1000; width: 960px; padding-top: 225px; float: left; clear: both; text-align: center; font-size: 20px;">@Model.displayName</div>
        <div style="position: absolute; z-index: 1000; width: 954px; padding-top: 350px; text-align: center; float: left; clear: both; font-size: 16px;">@Model.certificateDateStr</div>
    </div>

<script>


    window.print();
</script>
  


